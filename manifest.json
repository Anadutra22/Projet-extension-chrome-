{
    "update_url": "https://clients2.google.com/service/update2/crx",
    
        "manifest_version": 2,
    
        "name": "Clarify",
        "description": "A chrome extension to make websites more accessible for all users.",
        "version": "1.0",
        
        "icons": {
            "128": "icons/img_large2.jpg",
            "48": "icons/img_large2_small.jpg"
        },
    
            
        "browser_action": {
            "default_title": "Clarify",
            "default_popup": "interface.html",
            "default_icon": {
                "19": "icons/img_large_19.jpg",
                "38": "icons/img_large_38.jpg"
            }
        },
        
        
        
        "web_accessible_resources": [
            "icons/filters.svg"
        ],
        
        "content_scripts": [
            {
                "matches": ["<all_urls>"],
                "js": ["script/filters.js"],
                "css": []
                
            }
        ],
    
        "permissions": [
            "tabs",
            "http://*/*",
            "https://*/*",
            "activeTab"
        ]
    }